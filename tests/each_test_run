#!/bin/bash

failed=0

RED='\033[0;31m'
GREEN='\033[0;32m'
NC='\033[0m'

for i in isos profile random sample ni56 hydrostatic big_net;
do
    cd "$i" || exit 1
    if ./test 2>&1 >/dev/null; then
        echo -e "$i ${GREEN}passed${NC}"
    else
        echo -e "$i ${RED}failed${NC}"
        failed=1
    fi
    cd ../
done

exit $failed
